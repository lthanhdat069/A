repeat task.wait() until game:IsLoaded()

--============================
-- CONFIG
--============================
local Config = {
    GuiName = "JackUiPremium",
    ButtonImageId = "103173399598412", 
    WindowTitle = "JackUiPremium ðŸ’Ž",
    WindowSubTitle = "Dark Aqua Style by Sol",
    Theme = "Dark",
    WindowWidth = 660,
    WindowHeight = 390,
    TabHeight = 46,
    AnimationTime = 0.28,
    BackdropMaxTransparency = 0.6,
    TopBarIconSize = 20
}

--============================
-- USER SCRIPTS (Jack tá»± dÃ¡n)
--============================
local UserScripts = {
    Fly = "loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()",
    Invisible = "loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Awesome-Invisible-man-21074"))()",
    WalkOnAir = "loadstring(game:HttpGet("https://pastefy.app/Qc1ZIUwO/raw", true))()",
    InfiniteYield = "loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Infinite-Yield-43437"))()"
}

--============================
-- SERVICE SETUP
--============================
local TweenService = game:GetService("TweenService")
local CoreGui = game:GetService("CoreGui")
local UserInputService = game:GetService("UserInputService")

local function mk(parent, class, props)
    local obj = Instance.new(class)
    for k,v in pairs(props) do obj[k] = v end
    obj.Parent = parent
    return obj
end

local function tween(obj, props, time, style, dir)
    return TweenService:Create(obj, TweenInfo.new(time or Config.AnimationTime, style or Enum.EasingStyle.Quad, dir or Enum.EasingDirection.Out), props)
end

if CoreGui:FindFirstChild(Config.GuiName) then CoreGui[Config.GuiName]:Destroy() end

--============================
-- MAIN GUI
--============================
local ScreenGui = mk(CoreGui, "ScreenGui", {Name = Config.GuiName, ResetOnSpawn = false})

local Backdrop = mk(ScreenGui, "Frame", {
    BackgroundColor3 = Color3.fromRGB(0,0,0),
    BackgroundTransparency = 1,
    Size = UDim2.new(1,0,1,0),
    ZIndex = 2
})

local Window = mk(ScreenGui, "Frame", {
    BackgroundColor3 = Color3.fromRGB(18, 22, 28),
    Size = UDim2.new(0, Config.WindowWidth, 0, Config.WindowHeight),
    AnchorPoint = Vector2.new(0.5,0.5),
    Position = UDim2.new(1.2,0,0.5,0),
    ZIndex = 5
})
mk(Window, "UICorner", {CornerRadius = UDim.new(0,18)})

--============================
-- TITLE BAR
--============================
local TitleBar = mk(Window, "Frame", {Size = UDim2.new(1,0,0,54), BackgroundTransparency = 1})
local Title = mk(TitleBar, "TextLabel", {
    Text = Config.WindowTitle, Font = Enum.Font.GothamBold, TextSize = 20,
    TextColor3 = Color3.fromRGB(180,240,255),
    BackgroundTransparency = 1, Position = UDim2.new(0.03,0,0.1,0),
    TextXAlignment = Enum.TextXAlignment.Left
})
local SubTitle = mk(TitleBar, "TextLabel", {
    Text = Config.WindowSubTitle, Font = Enum.Font.Gotham, TextSize = 12,
    TextColor3 = Color3.fromRGB(130,180,200),
    BackgroundTransparency = 1, Position = UDim2.new(0.03,0,0.55,0),
    TextXAlignment = Enum.TextXAlignment.Left
})

local CloseBtn = mk(TitleBar, "TextButton", {
    Text = "âœ•", Size = UDim2.new(0,32,0,32), Position = UDim2.new(0.93,0,0.2,0),
    BackgroundColor3 = Color3.fromRGB(40,46,58), TextColor3 = Color3.fromRGB(180,240,255),
    Font = Enum.Font.GothamBold, TextSize = 16
})
mk(CloseBtn, "UICorner", {CornerRadius = UDim.new(0,8)})

-- Drag
local dragging, dragInput, dragStart, startPos
TitleBar.InputBegan:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = i.Position
        startPos = Window.Position
        i.Changed:Connect(function() if i.UserInputState == Enum.UserInputState.End then dragging = false end end)
    end
end)
TitleBar.InputChanged:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseMovement then dragInput = i end end)
UserInputService.InputChanged:Connect(function(i)
    if i == dragInput and dragging then
        local delta = i.Position - dragStart
        Window.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

--============================
-- TABS
--============================
local TabsBar = mk(Window, "Frame", {
    BackgroundTransparency = 1, Position = UDim2.new(0,0,0.13,0),
    Size = UDim2.new(1,0,0,Config.TabHeight)
})
local TabsLayout = mk(TabsBar, "UIListLayout", {
    FillDirection = Enum.FillDirection.Horizontal, Padding = UDim.new(0,6)
})

local Content = mk(Window, "Frame", {
    BackgroundTransparency = 1, Position = UDim2.new(0,12,0.13+(Config.TabHeight/Config.WindowHeight),0),
    Size = UDim2.new(1,-24,1,-(54+12+Config.TabHeight))
})

local function createTab(name, icon)
    local btn = mk(TabsBar, "TextButton", {
        Text = icon.." "..name,
        Font = Enum.Font.GothamBold, TextSize = 14,
        TextColor3 = Color3.fromRGB(180,240,255),
        Size = UDim2.new(0,110,0,Config.TabHeight),
        BackgroundColor3 = Color3.fromRGB(30,36,44)
    })
    mk(btn, "UICorner", {CornerRadius = UDim.new(0,10)})
    return btn
end

local Tabs = {"Main","Troll","Admin","Game","Fun","Extra"}
local Frames, Buttons = {},{}

for _,n in pairs(Tabs) do
    Buttons[n] = createTab(n, "â€¢")
    Frames[n] = mk(Content, "Frame", {Visible=false, BackgroundTransparency=1, Size=UDim2.new(1,0,1,0)})
    mk(Frames[n],"UIListLayout",{Padding=UDim.new(0,8)})
end

--============================
-- NÃšT CHá»¨C NÄ‚NG
--============================
local function addBtn(parent, title, desc, callback)
    local b = mk(parent, "TextButton", {
        BackgroundColor3 = Color3.fromRGB(36,44,56),
        Size = UDim2.new(1,0,0,44),
        Text = ""
    })
    mk(b, "UICorner", {CornerRadius = UDim.new(0,10)})
    mk(b,"TextLabel",{Text=title,Font=Enum.Font.GothamBold,TextSize=16,TextColor3=Color3.fromRGB(180,240,255),BackgroundTransparency=1,Position=UDim2.new(0.03,0,0,0),Size=UDim2.new(0.6,0,1,0),TextXAlignment=Enum.TextXAlignment.Left})
    mk(b,"TextLabel",{Text=desc,Font=Enum.Font.Gotham,TextSize=12,TextColor3=Color3.fromRGB(130,180,200),BackgroundTransparency=1,Position=UDim2.new(0.03,0,0.5,0),Size=UDim2.new(0.95,0,0.5,0),TextXAlignment=Enum.TextXAlignment.Left})
    b.MouseButton1Click:Connect(callback)
    return b
end

-- MAIN tab demo
addBtn(Frames["Main"], "Copy TikTok Jack", "Sao chÃ©p link TikTok cá»§a Jack", function()
    setclipboard("https://www.tiktok.com/@your_tiktok_here")
end)

--============================
-- TAB SWITCH
--============================
local current = nil
local function showTab(n)
    if current then Frames[current].Visible=false end
    Frames[n].Visible=true
    current = n
end
for n,b in pairs(Buttons) do b.MouseButton1Click:Connect(function() showTab(n) end) end
showTab("Main")

--============================
-- ANIMATION
--============================
local OpenBtn = mk(ScreenGui,"ImageButton",{
    Image="http://www.roblox.com/asset/?id="..Config.ButtonImageId,
    BackgroundTransparency=0.25,BackgroundColor3=Color3.fromRGB(0,0,0),
    Size=UDim2.new(0,48,0,48),Position=UDim2.new(0.06,0,0.12,0)
})
mk(OpenBtn,"UICorner",{CornerRadius=UDim.new(1,10)})

local open=false
local function pulse(b)
    local o=b.Size
    tween(b,{Size=o+UDim2.new(0,6,0,6)},0.12,Enum.EasingStyle.Sine,Enum.EasingDirection.Out):Play()
    task.wait(0.12)
    tween(b,{Size=o},0.12,Enum.EasingStyle.Sine,Enum.EasingDirection.In):Play()
end

local function toggle()
    pulse(OpenBtn)
    open = not open
    if open then
        Backdrop.Visible=true
        tween(Backdrop,{BackgroundTransparency=Config.BackdropMaxTransparency},Config.AnimationTime):Play()
        tween(Window,{Position=UDim2.new(0.5,0,0.5,0)},Config.AnimationTime,Enum.EasingStyle.Back,Enum.EasingDirection.Out):Play()
    else
        tween(Backdrop,{BackgroundTransparency=1},Config.AnimationTime):Play()
        tween(Window,{Position=UDim2.new(1.2,0,0.5,0)},Config.AnimationTime,Enum.EasingStyle.Back,Enum.EasingDirection.In):Play()
        task.wait(Config.AnimationTime)
        Backdrop.Visible=false
    end
end
OpenBtn.MouseButton1Click:Connect(toggle)
CloseBtn.MouseButton1Click:Connect(toggle)
Backdrop.InputBegan:Connect(function(i) if i.UserInputType==Enum.UserInputType.MouseButton1 then if open then toggle() end end end)
UserInputService.InputBegan:Connect(function(i,gp) if not gp and i.KeyCode==Enum.KeyCode.End then toggle() end end)

print("JackUiPremium ðŸ’Ž Dark Aqua loaded - by Sol ðŸ˜Ž")