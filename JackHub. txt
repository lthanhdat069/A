------------------------------------------------
-- TAB Troll: c√°c n√∫t vui/troll (v√≠ d·ª•, an to√†n)
------------------------------------------------
Tabs.Troll:AddButton({
	Title = "Make Player Float (demo)",
	Description = "Demo effect troll (kh√¥ng h·∫°i) - ch·ªçn m·ª•c ti√™u trong game ƒë·ªÉ √°p d·ª•ng",
	Callback = function()
		local plr = game.Players.LocalPlayer
		if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
			local hrp = plr.Character.HumanoidRootPart
			local old = hrp.Anchored
			hrp.Anchored = true
			task.delay(2, function() hrp.Anchored = old end)
			-- üëà ch·ªânh 2 -> s·ªë gi√¢y mu·ªën neo ng∆∞·ªùi ch∆°i
		else
			warn("Kh√¥ng t√¨m th·∫•y character ƒë·ªÉ demo effect.")
		end
	end
})

Tabs.Troll:AddButton({
	Title = "Fake Lag (local only)",
	Description = "Ch·ªâ l√† hi·ªáu ·ª©ng local ƒë·ªÉ g√¢y c∆∞·ªùi, kh√¥ng ·∫£nh h∆∞·ªüng server.",
	Callback = function()
		for i = 1, 6 do
			game:GetService("RunService").Stepped:Wait()
		end
		warn("Fake lag (local) completed.")
	end
})

