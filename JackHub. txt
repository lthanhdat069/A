repeat task.wait() until game:IsLoaded()
wait(1)

-- âš™ï¸ Chá»— Ä‘á»ƒ báº¡n dÃ¡n script tháº­t
local UserScripts = {
    Fly = "loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()", -- DÃ¡n script Fly cá»§a báº¡n vÃ o Ä‘Ã¢y
    Invisible = "loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Awesome-Invisible-man-21074"))()", -- DÃ¡n script Invisible vÃ o Ä‘Ã¢y
    WalkOnAir = "loadstring(game:HttpGet("https://pastefy.app/Qc1ZIUwO/raw", true))()", -- DÃ¡n script WalkOnAir vÃ o Ä‘Ã¢y
    InfiniteYield = "loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Infinite-Yield-43437"))()" -- DÃ¡n Infinite Yield á»Ÿ Ä‘Ã¢y
}

-- ğŸª© GUI chÃ­nh
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "JackUiPremium"
ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

-- ğŸ”˜ NÃºt má»Ÿ menu (Avatar)
local OpenButton = Instance.new("ImageButton")
OpenButton.Parent = ScreenGui
OpenButton.Size = UDim2.new(0, 60, 0, 60)
OpenButton.Position = UDim2.new(0.05, 0, 0.15, 0)
OpenButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
OpenButton.BorderSizePixel = 0
OpenButton.Image = "http://www.roblox.com/asset/?id=103173399598412" -- ğŸ‘ˆ Ä‘á»•i áº£nh avatar menu táº¡i Ä‘Ã¢y
OpenButton.AutoButtonColor = true
OpenButton.Active = true

local UICornerButton = Instance.new("UICorner", OpenButton)
UICornerButton.CornerRadius = UDim.new(1, 0)
local UIStrokeButton = Instance.new("UIStroke", OpenButton)
UIStrokeButton.Color = Color3.fromRGB(155, 210, 255)
UIStrokeButton.Thickness = 1.2

-- ğŸª Menu chÃ­nh
local MenuFrame = Instance.new("Frame")
MenuFrame.Size = UDim2.new(0, 550, 0, 350)
MenuFrame.Position = UDim2.new(0.5, -275, 0.5, -175)
MenuFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
MenuFrame.BorderSizePixel = 0
MenuFrame.Visible = false
MenuFrame.Active = true
MenuFrame.Draggable = true
MenuFrame.ClipsDescendants = true
MenuFrame.Parent = ScreenGui

local UICornerFrame = Instance.new("UICorner", MenuFrame)
UICornerFrame.CornerRadius = UDim.new(0, 20)
local UIStrokeFrame = Instance.new("UIStroke", MenuFrame)
UIStrokeFrame.Color = Color3.fromRGB(155, 210, 255)
UIStrokeFrame.Thickness = 1.2
local UIScale = Instance.new("UIScale", MenuFrame)
UIScale.Scale = 0

-- ğŸ’ TiÃªu Ä‘á»
local Title = Instance.new("TextLabel")
Title.Parent = MenuFrame
Title.Text = "JackUiPremium ğŸ’"
Title.Font = Enum.Font.GothamBold
Title.TextSize = 24
Title.TextColor3 = Color3.fromRGB(155, 210, 255)
Title.BackgroundTransparency = 1
Title.Position = UDim2.new(0, 20, 0, 10)
Title.Size = UDim2.new(1, -40, 0, 40)
Title.TextXAlignment = Enum.TextXAlignment.Left

-- ğŸ§­ Tabs ngang
local TabBar = Instance.new("Frame", MenuFrame)
TabBar.Size = UDim2.new(1, 0, 0, 40)
TabBar.Position = UDim2.new(0, 0, 0, 55)
TabBar.BackgroundTransparency = 0.15
TabBar.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
local TabCorner = Instance.new("UICorner", TabBar)
TabCorner.CornerRadius = UDim.new(0, 20)
local UIListLayout = Instance.new("UIListLayout", TabBar)
UIListLayout.FillDirection = Enum.FillDirection.Horizontal
UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
UIListLayout.Padding = UDim.new(0, 8)

-- ğŸšï¸ Tabs máº«u
local Tabs = {"Main 1", "Main 2", "Main 3", "Main 4"}
for _, name in pairs(Tabs) do
    local Button = Instance.new("TextButton")
    Button.Text = name
    Button.Parent = TabBar
    Button.Font = Enum.Font.Gotham
    Button.TextSize = 18
    Button.TextColor3 = Color3.fromRGB(155, 210, 255)
    Button.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    Button.Size = UDim2.new(0, 100, 1, -10)
    Button.AutoButtonColor = true
    Button.BackgroundTransparency = 0.2
    local BC = Instance.new("UICorner", Button)
    BC.CornerRadius = UDim.new(0, 12)
end

-- ğŸï¸ Animation má»Ÿ/Ä‘Ã³ng
local TweenService = game:GetService("TweenService")
local menuOpen = false

local function ToggleMenu()
	menuOpen = not menuOpen
	local tween = TweenService:Create(
		UIScale,
		TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
		{Scale = menuOpen and 1 or 0}
	)
	tween:Play()
	if menuOpen then
		MenuFrame.Visible = true
	else
		task.delay(0.4, function()
			if not menuOpen then
				MenuFrame.Visible = false
			end
		end)
	end
end

-- ğŸ”˜ Click vÃ o avatar Ä‘á»ƒ má»Ÿ/Ä‘Ã³ng menu
OpenButton.MouseButton1Click:Connect(function()
	ToggleMenu()
end)

print("âœ… JackUiPremium v3 loaded â€” Click vÃ o avatar Ä‘á»ƒ má»Ÿ menu ğŸ’")