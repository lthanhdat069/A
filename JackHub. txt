-- Jack Hub üíé - Full Hub template (Fluent UI)
-- Gi·ªØ nguy√™n giao di·ªán th·∫≠t, ch·ªâ th√™m h∆∞·ªõng d·∫´n ngay b√™n c·∫°nh d√≤ng c·∫ßn s·ª≠a (nh∆∞ y√™u c·∫ßu).
-- Sol created this for Jack. Ch√∫ √Ω: thay URL loadstring b·∫±ng ngu·ªìn b·∫°n tin t∆∞·ªüng.

repeat task.wait() until game:IsLoaded()

-- üü¢ N√∫t m·ªü menu ch√≠nh
local ScreenGui = Instance.new("ScreenGui")
local ImageButton = Instance.new("ImageButton")
local UICorner = Instance.new("UICorner")

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Name = "JackHubGUI" -- üëà ƒë·ªïi t√™n GUI n·∫øu mu·ªën (nh·ªõ tr√πng trong code n·∫øu ƒë·ªïi)

ImageButton.Parent = ScreenGui
ImageButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ImageButton.BorderSizePixel = 0
ImageButton.Position = UDim2.new(0.10615778, 0, 0.16217947, 0)
ImageButton.Size = UDim2.new(0, 40, 0, 40)
ImageButton.Draggable = true
ImageButton.Image = "http://www.roblox.com/asset/?id=103173399598412"  -- üëà ƒë·ªïi ID ·∫£nh n√∫t m·ªü menu t·∫°i ƒë√¢y (·∫£nh roblox)
ImageButton.Name = "OpenMenuButton" -- üëà ƒë·ªïi t√™n n√∫t n·∫øu mu·ªën

UICorner.CornerRadius = UDim.new(1, 10)
UICorner.Parent = ImageButton

-- M·ªü/ƒë√≥ng menu b·∫±ng ph√≠m End
ImageButton.MouseButton1Down:Connect(function()
    -- G·ª≠i s·ª± ki·ªán gi·∫£ ƒë·ªÉ minimize/maximize theo Fluent thi·∫øt l·∫≠p
	game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.End, false, game)
end)

-- üü£ T·∫£i Fluent UI (UI library)
-- N·∫øu b·∫°n mu·ªën s·ª≠ d·ª•ng th∆∞ vi·ªán Fluent kh√°c, ƒë·ªïi URL b√™n d∆∞·ªõi
local successFluent, Fluent = pcall(function()
    return loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
end)

if not successFluent then
    warn("Kh√¥ng th·ªÉ t·∫£i Fluent t·ª´ URL m·∫∑c ƒë·ªãnh. N·∫øu b·∫°n c√≥ th∆∞ vi·ªán UI kh√°c, thay URL trong m√£. (D√≤ng: loadstring Fluent)")
    -- B·∫°n c√≥ th·ªÉ d·ª´ng ·ªü ƒë√¢y ho·∫∑c ti·∫øp t·ª•c v·ªõi UI c∆° b·∫£n
end

repeat wait() until game:IsLoaded()

-- ü™ü C·ª≠a s·ªï ch√≠nh
local Window = Fluent:CreateWindow({
	Title = "Jack Hub üíé",         -- üëà ƒë·ªïi th√†nh t√™n script c·ªßa b·∫°n
	SubTitle = "Script vip x·ªãn nh·∫•t t·ª´ Jack", -- üëà ƒë·ªïi m√¥ t·∫£ ph·ª•
	TabWidth = 157,
	Size = UDim2.fromOffset(450, 300),
	Acrylic = true,
	Theme = "Aqua",                   -- üëà ƒë·ªïi m√†u ch·ªß ƒë·∫°o: Amethyst, Dark, Aqua, Light, Rose, Emerald, Crimson, Ocean
	MinimizeKey = Enum.KeyCode.End
})

-- üìë T·∫°o 6 tab m·∫∑c ƒë·ªãnh (theo c·∫•u h√¨nh b·∫°n x√°c nh·∫≠n)
local Tabs = {
	Main    = Window:AddTab({ Title = "Main" }),   -- üëà ƒë·ªïi t√™n tab n·∫øu mu·ªën
	Troll   = Window:AddTab({ Title = "Troll" }),
	Admin   = Window:AddTab({ Title = "Admin" }),
	Game    = Window:AddTab({ Title = "Game" }),
	Fun     = Window:AddTab({ Title = "Fun" }),
	Extra   = Window:AddTab({ Title = "Extra" })
}

------------------------------------------------
-- TAB Main: C√°c script c∆° b·∫£n / ti·ªán √≠ch
------------------------------------------------
Tabs.Main:AddButton({
	Title = "Copy TikTok Jack",              -- üëà t√™n n√∫t
	Description = "Sao ch√©p link TikTok c·ªßa Jack",-- üëà m√¥ t·∫£ n√∫t
	Callback = function()
		setclipboard("https://www.tiktok.com/@your_tiktok_here")  -- üëà thay b·∫±ng link TikTok c·ªßa b·∫°n
	end
})

