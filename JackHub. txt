------------------------------------------------
-- TAB Fun: bay, noclip, invis (v√≠ d·ª• an to√†n / placeholder)
------------------------------------------------
Tabs.Fun:AddButton({
	Title = "Fly (local demo)",
	Description = "Demo fly local (kh√¥ng d√πng exploit m·∫°ng). Thay b·∫±ng script ƒë·∫ßy ƒë·ªß n·∫øu mu·ªën.",
	Callback = function()
		local plr = game.Players.LocalPlayer
		if not plr or not plr.Character then return end
		local humanoid = plr.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid.PlatformStand = true
			task.delay(2, function() humanoid.PlatformStand = false end)
			-- üëà 2 gi√¢y demo: ch·ªânh th·ªùi gian n·∫øu c·∫ßn
		end
	end
})

Tabs.Fun:AddButton({
	Title = "Noclip (toggle local)",
	Description = "Noclip local-demo: toggle CanCollide. Ch·ªâ demo local.",
	Callback = function()
		local plr = game.Players.LocalPlayer
		if not plr or not plr.Character then return end
		for _,v in pairs(plr.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = not v.CanCollide
			end
		end
		warn("Toggled local noclip CanCollide state. (Demo)")
	end
})

Tabs.Fun:AddButton({
	Title = "Invisible (local)",
	Description = "L√†m m·ªù local avatar (demo).",
	Callback = function()
		local plr = game.Players.LocalPlayer
		if not plr or not plr.Character then return end
		for _,v in pairs(plr.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.Transparency = 1
			elseif v:IsA("Decal") then
				v.Transparency = 1
			end
		end
		task.delay(3, function()
			-- Khi demo xong, reset transparency v·ªÅ 0 (n·∫øu mu·ªën)
			-- L∆∞u √Ω: n·∫øu avatar c√≥ cosmetics, c√≥ th·ªÉ c·∫ßn x·ª≠ l√Ω kh√°c
			for _,v in pairs(plr.Character:GetDescendants()) do
				if v:IsA("BasePart") then
					v.Transparency = 0
				elseif v:IsA("Decal") then
					v.Transparency = 0
				end
			end
		end)
	end
})

------------------------------------------------
-- TAB Extra: li√™n k·∫øt, h∆∞·ªõng d·∫´n, c·∫≠p nh·∫≠t
------------------------------------------------
Tabs.Extra:AddButton({
	Title = "H∆∞·ªõng d·∫´n ch·ªânh s·ª≠a script",
	Description = "M·ªü clipboard v·ªõi h∆∞·ªõng d·∫´n c∆° b·∫£n",
	Callback = function()
		local guide = [[H∆∞·ªõng d·∫´n nhanh ch·ªânh s·ª≠a Jack Hub:
- ƒê·ªïi t√™n menu: s·ª≠a Title trong CreateWindow (d√≤ng Window:CreateWindow).
- ƒê·ªïi ·∫£nh n√∫t m·ªü menu: thay ID ·ªü ImageButton.Image.
- Th√™m script: t·∫°i c√°c n√∫t placeholder, thay loadstring(...) b·∫±ng URL raw script c·ªßa b·∫°n.
- Lu√¥n ki·ªÉm tra ngu·ªìn script tr∆∞·ªõc khi ch·∫°y.
]]
		setclipboard(guide)
		warn("H∆∞·ªõng d·∫´n ƒë√£ ƒë∆∞·ª£c sao ch√©p v√†o clipboard.")
	end
})

Tabs.Extra:AddButton({
	Title = "Sao ch√©p ID n√∫t",
